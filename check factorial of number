public class factorial {
    static int fact(int n){
        
        // if (n==0)
        // return 1;
        // else
        // return n*fact(n-1);

        //or
        
        return (n==0 ||n==1)?1:n*fact(n-1);
    }
    public static void main(String[] args){
        int num=5;
        System.out.println("factorial of number is "+fact(num));
    }
}
